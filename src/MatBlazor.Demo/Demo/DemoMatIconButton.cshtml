@using MatBlazor.Models
@using Microsoft.JSInterop


<h2>MatIconButton</h2>

<DemoContainer>
    <Content>
        <h5>Simple use</h5>
        <MatIconButton Icon="favorite" OnClick="@RunOnClick"></MatIconButton>
        
        <h5>With Font-Awsome Icons</h5>
        <MatIconButton>
            <i class="fa fa-github" aria-hidden="true"></i>
        </MatIconButton>
        
        <h5>With href and title</h5>
        <MatIconButton  HRef="https://github.com/BlazorComponents/MatBlazor" Title="MatBlazor GitHub" Target="_blank">
            <MatIcon>@MatIconNames.Touch_app</MatIcon>
        </MatIconButton>

        @functions
        {

            public void RunOnClick(UIMouseEventArgs e)
            {
                JSRuntime.Current.InvokeAsync<object>("window.alert", "Test");
            }

        }

    </Content>
