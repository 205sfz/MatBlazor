@namespace MatBlazor
@typeparam T
@inherits BaseMatDatePickerType<T>

@base.BuildRenderTree

@code
{


    protected override RenderFragment GetChildContent()
    {
        return (builder) =>
        {
            builder.AddContent(0, base.GetChildContent());
            builder.AddContent(1, @<div class="mat-date-picker-icon-container">
                                      <input @ref="flatpickrInputRef" style="display: none;"/>
                                      <MatIconButton Icon="@MatIconNames.Calendar_today" OnClick="OnClickIconHandler"></MatIconButton>
                                  </div>);
        };
    }

}